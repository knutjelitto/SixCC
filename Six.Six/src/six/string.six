namespace six.core;

shared native class String
{
    shared int Length => 1;

    shared static String Concat(String s1, String s2)
    {
        if (s1.Length == 0)
        {
            return s2;
        }
        else if (s2.Length == 0)
        {
            return s1;
        }
        else
        {
            return "s1-concat-s2";
        }
    }
}

shared alias string => String;
