namespace six.core;

class Complex
{
    var double re = -1;
    var double im = -1;

    shared new (double re = 0, double im = 0)
    {
        self.re = re;
        self.im = im;
    }

    shared infix Complex + (Complex other)
    {
        return Complex(self.re + other.re, self.im + other.im);
    }
}
