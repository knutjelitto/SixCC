namespace six.core;

shared sealed class Complex
{
    var int re = 0;
    var int im = 0;

    shared new (int re = 0, int im = 0)
    {
        self.re = re;
        self.im = im;
    }

    shared int Re => self.re;
    shared int Im => self.im;

    shared infix Complex + (Complex other)
    {
        return Complex(self.re + other.re, self.im + other.im);
    }
}
