namespace six;

shared alias bool => Boolean;

shared builtin Boolean of true | false
{
    shared prefix Boolean !  () => builtin;
    shared infix Boolean | (Boolean rhs) => builtin;
    shared infix Boolean & (Boolean rhs) => builtin;
    shared infix Boolean ^ (Boolean rhs) => builtin;
}

shared bool true => builtin;
shared bool false => builtin;

