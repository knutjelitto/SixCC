namespace six.core;

shared alias bool => Boolean;

shared primitive Boolean of true | false
{
    shared native prefix Boolean !  ();
    shared native infix Boolean | (Boolean rhs);
    shared native infix Boolean & (Boolean rhs);
    shared native infix Boolean ^ (Boolean rhs);
}

shared object true : Boolean
{
    int hash => 1234;
    string string => "true";
}

shared object false : Boolean
{
    int hash => 4321;
    string string => "false";
}

