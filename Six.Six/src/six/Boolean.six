namespace six;

shared alias bool => Boolean;

shared builtin Boolean of true | false
{
    shared operator Boolean _|_ (Boolean rhs) => builtin;
    shared operator Boolean _&_ (Boolean rhs) => builtin;
    shared operator Boolean _^_ (Boolean rhs) => builtin;
    shared operator Boolean !_ () => builtin;
}

shared Boolean true => builtin;
shared Boolean false => builtin;

