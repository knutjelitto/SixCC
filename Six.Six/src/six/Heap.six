namespace six.core;

shared let uint __data_start = 0;
shared let uint __data_end = 0;
shared let uint __heap_start = 0;
shared var uint __heap_next = 0;

shared uint allocate(uint size)
{
    let allocated = (__heap_next + 15u32) & #FFFFFFF0u32;
    __heap_next = allocated + size;
    return allocated;
}

