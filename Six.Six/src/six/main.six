namespace six;

shared class Point
{
    shared int x = -1;
    shared int y = -1;

    shared new ();

    shared int sum()
    {
        return x + y;
    }

    shared void moveX(int dx)
    {
        x = x + dx;
    }
}

void main()
{
    let x = -12;
    let y = -13;

    var q = Point();
    var p = q;
    p.x = x;
    p.y = y;

    print(p.sum());
}