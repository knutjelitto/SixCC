namespace six;

shared builtin Boolean
{
    shared operator bool _|_ (bool rhs) => builtin;
    shared operator bool _&_ (bool rhs) => builtin;
    shared operator bool _^_ (bool rhs) => builtin;
    shared operator bool !_ () => builtin;
}

shared alias bool => Boolean;

shared bool true => builtin;
shared bool false => builtin;

