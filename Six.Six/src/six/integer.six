namespace six;

builtin i8;
builtin i16;
builtin i32;
builtin i64;

builtin s16;
builtin s32;
builtin s64;

builtin u16;
builtin u32;
builtin u64;

alias byte => i8;

alias short => s16;
alias ushort => u16;

alias int => s32;
alias uint => u32;

alias long => s64;
alias ulong => u64;

infix uint +(uint left, uint right) => builtin;
infix int +(int left, int right) => builtin;
